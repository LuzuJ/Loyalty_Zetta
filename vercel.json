{
  "version": 2,
  "builds": [
    {
      "src": "public/server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": "src/**/*.ts"
      }
    }
  ],
  "routes": [
    {
      "src": "/api-docs",
      "dest": "/src/server.ts"
    },
    {
      "src": "/api-docs/(.*)",
      "dest": "/src/server.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/src/server.ts"
    }
  ]
}